<!DOCTYPE html>
<html>
  <head>
    <title>Food</title>
  </head>
  <body>

    Welcome to the food route.

    <table border="1">
      <% for (var i = 0; i < food.length; i++) { %>
        <tr>
          <td> <%= food[i].name %> </td>
          <td> <%= food[i].quantity %> </td>
          <td> <a href="/food/<%= food[i].id %>/edit">Edit This Food</a> </td>
          <td> <a href="/food/<%= food[i].id %>/delete">Delete This Food</a> </td>
        </tr>
      <% } %>
    </table>

    <p>
      <a href="/food/new"><button type="button" name="button">Add a New Food</button></a>
    </p>

  </body>
</html>
